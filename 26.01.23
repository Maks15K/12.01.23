}

int main() {
	int pro = 0;
	cout << "enter your procent - "<< endl;
	cin >> pro;
	if (pro >= 90) {
		cout << "  Grade A" << endl;
	}
	else if (pro >= 80) {
		cout << "  Grade B" << endl;
	}
	else if (pro >= 70) {
		cout << "  Grade C" << endl;
	}
	else if (pro >= 60) {
		cout << "  Grade D" << endl;
	}
	else if (pro >= 40) {
