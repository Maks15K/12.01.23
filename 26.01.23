int main() {
	int x = 10;
	switch (x)
	{
	case 10:
		cout << "x is 10"; break;
	case 20:
		cout << "x is 20"; break;
	case 30:
		cout << "x is 30"; break;
	default:
		cout << "x is not 10, 20 or 30"; break;
	}
	return 0;
}

int main() {
	int x, x1;
	cout << "ievadiet pirmo skaitli" << endl;
	cin >> x;
	cout << "ievadiet otro skaitli" << endl;
	cin >> x1;
	if (x1 < x) {
		cout << "x ir max bet x1 ir min"<< endl;
	}
	else if (x1 > x) {
		cout << "x ir min bet x1 ir max"<<endl;
	}
	else {
		cout << "skaitļi ir vienadi" << endl;
	}
	if (x < 0 && x1 < 0) {
		cout << "skaitļi ir nagativi" << endl;
	}
