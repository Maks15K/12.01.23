int main() {
	int side1, side2, side3;

	cout << "Enter triangle first side :" << endl;
	cin >> side1;
	cout << "Enter triangle second side :" << endl;
	cin >> side2;
	cout << "Enter triangle third side :" << endl;
	cin >> side3;

	if (side1 == side2 && side1 == side3) {
		cout << "triangle is regular" << endl;
	}
	else if (side1 == side2 || side1 == side3 || side2 == side3) {
		cout << "triangle is side_equal" << endl;
	}
}

void checkIfOdd(int num) {
	if (num % 2 == 1) {
		cout << "number is odd" << endl;
	}
	else {
		cout << "number is not odd" << endl;
	}
}

int main() {
	int num;
	cout << "input number" << endl;
	cin >> num;
	checkIfOdd(num);
}


