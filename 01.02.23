int main() {
	int x = 10;
	switch (x)
	{
	case 10:
		cout << "x is 10"; break;
	case 20:
		cout << "x is 20"; break;
	case 30:
		cout << "x is 30"; break;
	default:
		cout << "x is not 10, 20 or 30"; break;
	}
	return 0;
}

